#include<bits/stdc++.h>
using namespace std;


int main(){
    //write your code here
    int n;
    cin>>n;
    vector<int>arr(n);
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int d;
    cin>>d;

    int idx=-1;
    for(int i=0;i<n;i++){
        if(arr[i]==d){
            idx=i;
            break;
        }
    }
    cout<<idx<<endl;
    return 0;
}
